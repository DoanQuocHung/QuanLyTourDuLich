@page
@model WebForm.Pages.Shared.QuanLyChiTietTour_SuaModel
@{
    ViewData["Title"] = "Website Quản Lý Tour";
    Layout = "_Layout";
}

<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
            <div class="col-12 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Thêm Chi Tiết Tour</h4>
                        <form class="forms-sample">
                            <div class="form-group">
                                <label for="exampleInputName1">Mã Tour</label>
                                <input type="text" class="form-control" placeholder="Mã Tour">
                            </div>
                            <div class="form-group">
                                <label for="exampleInputName1">Địa Điểm</label>
                                <input type="text" class="form-control" placeholder="Địa Điểm">
                            </div>
                            <div class="form-group">
                                <label for="exampleInputName1">Thứ tự</label>
                                <input type="text" class="form-control" placeholder="Thứ tự">
                            </div>
                            <button type="submit" class="btn btn-primary mr-2">Sửa</button>
                            <button class="btn btn-light">Hủy</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>