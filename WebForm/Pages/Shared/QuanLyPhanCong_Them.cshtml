@page
@model WebForm.Pages.Shared.QuanLyPhanCong_ThemModel
@{
    ViewData["Title"] = "Website Quản Lý Tour";
    Layout = "_Layout";
}
@using BUS;
@{
    PhanCongBUS bus = new PhanCongBUS();
}
<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
            <div class="col-12 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Thêm Phân Công</h4>
                        <form class="forms-sample">
                            <div class="form-group">
                                <label for="exampleInputName1">Mã Đoàn</label>
                                <input type="text" class="form-control" placeholder="Mã Đoàn">
                            </div>
                            <div class="form-group">
                                <label for="exampleInputName1">Mã Nhân Viên</label>
                                <input type="text" class="form-control" placeholder="Mã Nhân Viên">
                            </div>
                            <div class="form-group">
                                <label for="exampleInputName1">Nhiệm vụ</label>
                                <input type="text" class="form-control" placeholder="Nhiệm vụ">
                            </div>
                            <button type="submit" class="btn btn-primary mr-2">Thêm</button>
                            <button class="btn btn-light">Hủy</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>