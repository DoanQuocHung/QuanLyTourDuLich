@page
@model WebForm.Pages.Shared.QuanLyTour_ThemModel
@{
    ViewData["Title"] = "Website Quản Lý Tour";
    Layout = "QuanLyTour";
}

<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
            <div class="col-12 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Thêm Tour</h4>
                        <form class="forms-sample">
                            <div class="form-group">
                                <label for="exampleInputName1">Mã Tour</label>
                                <input type="text" class="form-control" placeholder="Mã Tour">
                            </div>
                            <div class="form-group">
                                <label for="exampleInputName1">Tên Tour</label>
                                <input type="text" class="form-control" placeholder="Tên Tour">
                            </div>
                            <div class="form-group">
                                <label for="exampleInputName1">Đặc điểm</label>
                                <input type="text" class="form-control" placeholder="Đặc điểm">
                            </div>
                            <div class="form-group">
                                <label for="exampleInputName1">Mã Loại</label>
                                <input type="text" class="form-control" placeholder="Mã Loại">
                            </div>
                            <button type="submit" class="btn btn-primary mr-2">Thêm</button>
                            <button class="btn btn-light">Hủy</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>