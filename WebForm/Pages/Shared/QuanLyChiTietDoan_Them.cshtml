@page
@model WebForm.Pages.Shared.QuanLyChiTietDoan_ThemModel
@{
    ViewData["Title"] = "Website Quản Lý Tour";
    Layout = "_Layout";
}

<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
            <div class="col-12 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Thêm Chi Tiết Đoàn</h4>
                        <form class="forms-sample">
                            <div class="form-group">
                                <label for="exampleInputName1">Mã Đoàn</label>
                                <input type="text" class="form-control" placeholder="Mã Đoàn">
                            </div>
                            <div class="form-group">
                                <label for="exampleInputName1">Mã Khách</label>
                                <input type="text" class="form-control" placeholder="Mã Khách">
                            </div>
                            <div class="form-group">
                                <label for="exampleInputName1">Họ tên</label>
                                <input type="text" class="form-control" placeholder="Họ tên">
                            </div>
                            <div class="form-group">
                                <label for="exampleInputName1">CMND</label>
                                <input type="text" class="form-control" placeholder="CMND">
                            </div>
                            <button type="submit" class="btn btn-primary mr-2">Thêm</button>
                            <button class="btn btn-light">Hủy</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>